{
  "name": "vso-team-calendar",
  "version": "2.0.0",
  "description": "Track events important to your team, view and manage days off, quickly see when sprints start and end, and more.",
  "keywords": [
    "vsts",
    "tfs",
    "extension",
    "calendar",
    "extensions",
    "Azure DevOps",
    "Visual Studio Team Services"
  ],
  "author": "Microsoft",
  "contributors": [
